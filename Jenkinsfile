pipeline {
    agent any

    stages {
        stage('Docker Login') {
            steps {
                echo 'Logging in..'
            }
        }
        stage('Download GIT Hub Repo') {
            steps {
                echo 'Downloading..'
            }
        }
        stage('Build') {
            steps {
                echo 'Building....'
            }
        stage('Scan') {
            steps {
                echo 'Scanning....'
            }
        stage('Push to ECR') {
            steps {
                echo 'Pushing to ECR....'
            }
        
        }
    }
}